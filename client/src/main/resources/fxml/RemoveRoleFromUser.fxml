<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.RemoveRoleFromUserController">
    <children>
        <Label text="Remove Role From User">
            <font>
                <Font size="25.0" />
            </font>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
      <ChoiceBox fx:id="userCB" prefWidth="150.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </ChoiceBox>
      <ChoiceBox fx:id="roleCB" prefWidth="150.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </ChoiceBox>
        <Button fx:id="removeRoleFromUser" mnemonicParsing="false" onAction="#removeSelected" prefWidth="110.0" text="Remove Role">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Button>
      <Label layoutX="359.0" layoutY="30.0" text="Just for you fred">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
    </children>
</VBox>
