            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
               <children>
            
                    <fx:include fx:id="messageView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="MessageView.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
               <children>
                    <fx:include fx:id="sendMessage" source="SendMessage.fxml" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
      <SplitPane dividerPositions="0.1304" minWidth="500.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="500.0">
        <items>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
