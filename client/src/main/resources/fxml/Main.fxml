                    <tabs>
                      <Tab closable="false" text="Chats">
                        <content>
                            <fx:include fx:id="chatList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="ChatList.fxml" />
                        </content>
                      </Tab>
                      <Tab closable="false" text="Users">
                        <content>
                            <fx:include fx:id="userList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="UserList.fxml" />
                        </content>
                      </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
    </children>
</HBox>
