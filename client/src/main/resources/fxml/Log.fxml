<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.LogController">
   <children>
      <VBox>
         <children>
            <HBox alignment="TOP_CENTER" prefWidth="1000.0">
               <children>
                  <Button fx:id="allBtn" visible="false" onAction="#allLogs" mnemonicParsing="false" text="All Logs">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="customBtn" visible="false" onAction="#customLogs" mnemonicParsing="false" text="Custom Logs">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <TableView visible="false" fx:id="table" prefHeight="493.0" prefWidth="1000.0">
              <columns>
                <TableColumn fx:id="logLevel" prefWidth="115.0" text="Log Level" />
                <TableColumn fx:id="message" prefWidth="640.8768005371094" text="Message" />
                  <TableColumn fx:id="timestamp" prefWidth="243.1231689453125" text="Timestamp" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
