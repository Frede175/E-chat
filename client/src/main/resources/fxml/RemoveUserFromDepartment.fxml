<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root" alignment="TOP_CENTER" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.RemoveUserFromDepartmentController">
    <children>
        <Label text="Remove user from department">
            <font>
                <Font size="25.0" />
            </font>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
        <ComboBox fx:id="selectDepartment" prefWidth="300.0" promptText="Department">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </ComboBox>
        <ComboBox fx:id="selectUser" prefHeight="26.0" prefWidth="300.0" promptText="User">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </ComboBox>
        <Button fx:id="removeButtonid" mnemonicParsing="false" onAction="#removeUserFromDepartment" text="Remove">
            <VBox.margin>
                <Insets bottom="20.0" top="20.0" />
            </VBox.margin></Button>
    </children>
</VBox>
