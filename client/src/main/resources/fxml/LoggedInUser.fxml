<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" prefHeight="70.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.LoggedInUserController">
   <children>
      <Label fx:id="userNameL" alignment="CENTER" prefHeight="17.0" prefWidth="254.0" text="Lars ">
         <font>
            <Font size="25.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Button fx:id="adminBtn" onAction="#openAdmin" mnemonicParsing="false" text="Admin" />
      <Button mnemonicParsing="false" onAction="#logout" text="Logout">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin></Button>
   </children>
</HBox>
