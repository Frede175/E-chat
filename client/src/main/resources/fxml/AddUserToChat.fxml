<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.AddUserToChatController">
   <children>
      <Label text="Add user to chat">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <ComboBox fx:id="usersCB" prefWidth="300.0" promptText="User">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </ComboBox>
      <ComboBox fx:id="chatsCB" prefWidth="300.0" promptText="Chat">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </ComboBox>
      <Button fx:id="addUser" mnemonicParsing="false" onAction="#addUser" text="Add">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
