<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.AddRoleToUserController">
    <children>
        <Label text="Add Role to User">
            <font>
                <Font size="25.0" />
            </font>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
      <ComboBox fx:id="selectUser" prefWidth="150.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </ComboBox>
      <ComboBox fx:id="selectRole" prefWidth="150.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </ComboBox>
        <Button fx:id="addRoleToUser" mnemonicParsing="false" onAction="#addSelected" text="Add Role">
            <VBox.margin>
                <Insets bottom="20.0" top="20.0" />
            </VBox.margin></Button>
    </children>
</VBox>
