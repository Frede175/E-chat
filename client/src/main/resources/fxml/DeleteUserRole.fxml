<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.DeleteUserRoleController">
   <Label text="Delete User Role">
      <VBox.margin>
         <Insets top="20.0" />
      </VBox.margin>
      <font>
         <Font size="20.0" />
      </font>
   </Label>
   <ComboBox fx:id="selectRole" prefWidth="150.0">
      <VBox.margin>
         <Insets bottom="20.0" top="20.0" />
      </VBox.margin>
   </ComboBox>
    <Button fx:id="deleteRole" mnemonicParsing="false" onAction="#deleteRole" text="Delete Role">
        <VBox.margin>
            <Insets bottom="20.0" />
        </VBox.margin>
    </Button>
</VBox>
