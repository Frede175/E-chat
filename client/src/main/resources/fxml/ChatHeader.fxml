<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx" maxWidth="1.7976931348623157E308" xmlns:fx="http://javafx.com/fxml" fx:controller="GUI.Controller.ChatHeaderController">
   <children>
      <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" layoutY="-36.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="chatNameL" alignment="CENTER" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" prefHeight="30.0" textAlignment="CENTER">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Button fx:id="cHButton" mnemonicParsing="false" onAction="#usersInChatBtn" text="Users">
               <HBox.margin>
                  <Insets left="20.0" right="10.0" />
               </HBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button fx:id="leaveBtn" mnemonicParsing="false" onAction="#leaveChat" text="Leave" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
