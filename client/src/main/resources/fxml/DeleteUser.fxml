<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="GUI.Controller.DeleteUserController">
    <children>
        <Label text="Delete User">
            <font>
                <Font size="25.0" />
            </font>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
        <ComboBox fx:id="selectUser" prefWidth="300.0" promptText="User">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </ComboBox>
        <Button fx:id="deleteButtonid" mnemonicParsing="false" onAction="#deleteUser" text="Delete">
            <VBox.margin>
                <Insets bottom="20.0" top="20.0" />
            </VBox.margin></Button>
    </children>
</VBox>
